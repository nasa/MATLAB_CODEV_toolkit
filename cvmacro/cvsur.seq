!archive code
for ^i 0 (num s)
	buf put b1 i^i+1 j1 (rdy s^i)
	buf put b1 i^i+1 j2 (rdx s^i)
	buf put b1 i^i+1 j3 (thi s^i)
	buf put b1 i^i+1 j4 (map s^i)
    if (typ sur s^i)<>"SPH"
    	buf put b1 i^i+1 j5 (k s^i)
    els
    	buf put b1 i^i+1 j5 0
    end if
	buf put b1 i^i+1 j6 (typ sur s^i)
	buf put b1 i^i+1 j7 (gla s^i)
	buf put b1 i^i+1 j8 (rmd s^i)
	buf put b1 i^i+1 j9 (sll s^i)
	buf put b1 i^i+1 j10 (typ dec s^i)
	buf put b1 i^i+1 j11 (xde s^i)
	buf put b1 i^i+1 j12 (yde s^i)
	buf put b1 i^i+1 j13 (zde s^i)
	buf put b1 i^i+1 j14 (ade s^i)
	buf put b1 i^i+1 j15 (bde s^i)
	buf put b1 i^i+1 j16 (cde s^i)
	buf put b1 i^i+1 j17 (xod s^i)
	buf put b1 i^i+1 j18 (yod s^i)
	buf put b1 i^i+1 j19 (zod s^i)
	buf put b1 i^i+1 j20 (mav s^i)
	buf put b1 i^i+1 j21 (dum s^i)

end for
 